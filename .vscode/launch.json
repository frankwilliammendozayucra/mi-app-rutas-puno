{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Streamlit: Run app",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/venv/bin/streamlit",
            "args": [
                "run",
                "streamlit_app.py"
            ],
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            }
        }
    ]
}